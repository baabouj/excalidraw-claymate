{
  "name": "excalidraw-claymate",
  "version": "1.0.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@excalidraw/excalidraw": "0.15.2",
    "@vitejs/plugin-react": "^4.0.0",
    "browser-fs-access": "^0.33.1",
    "excalidraw-animate": "0.6.0",
    "gif.js": "^0.2.0",
    "lodash": "^4.17.21",
    "nanoid": "3.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "svg-to-excalidraw": "0.0.2",
    "vite": "^4.3.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.194",
    "@types/nanoid": "^3.0.0",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.3",
    "canvas": "^2.11.2",
    "cypress": "^12.11.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "yarn run test:all",
    "test:all": "yarn run test:type && yarn run test:lint && yarn run test:format",
    "test:type": "tsc",
    "test:lint": "eslint --max-warnings=0 --ext .js,.ts,.tsx .",
    "test:format": "prettier 'src/*.(js|ts|tsx|css|scss)' --list-different",
    "e2e": "cypress open"
  },
  "eslintConfig": {
    "extends": [
      "plugin:cypress/recommended"
    ],
    "plugins": [
      "cypress"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|sass)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(excalidraw-animate|browser-fs-access)/)"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
